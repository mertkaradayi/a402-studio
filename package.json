{
  "name": "a402-inspector",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "dev:web": "turbo dev --filter=web",
    "dev:api": "turbo dev --filter=api",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=20"
  }
}